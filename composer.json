{
    "name": "rocketgate/sdk",
    "description": "Client PHP",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RocketGate\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RocketGate\\Sdk\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6",
        "ext-xml": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "suin/phpcs-psr4-sniff": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
